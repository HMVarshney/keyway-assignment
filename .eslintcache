104"],"import React, { useContext, useEffect, useState } from 'react';\nimport { getPost } from '../../api/getPosts';\nimport PostCard from '../../components/posts/PostCard';\nimport { AuthContext } from '../../context/authenticationContext/authContext';\n\n\nconst PostDisplay = (props) => {\n\n    const { authStatus } = useContext(AuthContext);\n\n    const [post, setPost] = useState({\n        loading: false,\n        post: {},\n        error: null\n    });\n\n    useEffect(() => {\n        setPost((current) => ({ ...current, loading: true }));\n        (async () => {\n            const response = await getPost(props.match.params.id, authStatus.jwt);\n\n            if (response?.status === 200) {\n                setPost({ loading: false, post: response.data, error: null });\n                return;\n            }\n\n            setPost({ loading: false, posts: [], error: response });\n            return;\n        })();\n    }, []);\n\n    // console.log(props.match.params);\n\n    if (post.loading) {\n        return (<div>Loading....</div>)\n    }\n\n    if (post.error) {\n        return (<div><h2>There has been an error fetching the post.</h2></div>)\n    }\n\n    return (\n        <div className='container'>\n            <div className='d-flex flex-column p-4'>\n                <h2>{post.post.title}</h2>\n                <div className='mt-4 col-9'>\n                    <img style={{ borderRadius: '8px' }} src='/assets/images/blog.jpg' alt='blog' width='100%' />\n                </div>\n                <div className='mt-4'>\n                    <p>{post.post.content}</p>\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default PostDisplay;","/home/hm/Documents/Web Development/Keyway Solutions/assignment/frontend/src/components/navbar/Navbar.js",["105","106"],"import Button from '@bit/semantic-org.semantic-ui-react.button';\nimport React, { useContext } from 'react';\nimport { AuthContext } from '../../context/authenticationConte